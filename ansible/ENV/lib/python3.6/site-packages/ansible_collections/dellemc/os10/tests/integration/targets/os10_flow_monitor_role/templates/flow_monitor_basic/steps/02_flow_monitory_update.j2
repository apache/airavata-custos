os10_flow_monitor:
  session 1:
    session_type: local
    description: "session1 desc"
    port_match:
      - interface_name: {{ os10_interface_1 }}
        location: source
        state: present
      - interface_name: {{ os10_interface_2 }}
        location: destination
        state: present
    flow_based: true
    shutdown: down
    state: present
  session 2:
    state: present
